{
	"files.associations": {
		"wrangler.json": "jsonc"
	},
	"typescript.tsdk": "node_modules/typescript/lib",
	"typescript.tsserver.web.projectWideIntellisense.enabled": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}